{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Covid19-DE"
		},
		"ls_ablob_covid19itisa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covid19itisa'"
		},
		"ls_adls_covid19itidl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covid19itidl'"
		},
		"ls_db_covid_cluster_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_db_covid_cluster'"
		},
		"ls_sql_covid_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_covid_db'"
		},
		"Is_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"ls_adls_covid19itidl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid19itidl.dfs.core.windows.net/"
		},
		"ls_db_covid_cluster_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1206-212628-ffud3dgx"
		},
		"tr_population_data_arrived_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/9ef7084d-1000-4031-b8cb-9ccdc1a3f173/resourceGroups/Covid19/providers/Microsoft.Storage/storageAccounts/covid19itisa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Covid19-DE_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covid19itisa_connectionString": {
						"value": "[parameters('ls_ablob_covid19itisa_connectionString')]"
					},
					"ls_adls_covid19itidl_accountKey": {
						"value": "[parameters('ls_adls_covid19itidl_accountKey')]"
					},
					"ls_db_covid_cluster_accessToken": {
						"value": "[parameters('ls_db_covid_cluster_accessToken')]"
					},
					"ls_sql_covid_db_connectionString": {
						"value": "[parameters('ls_sql_covid_db_connectionString')]"
					},
					"Is_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('Is_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"ls_adls_covid19itidl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covid19itidl_properties_typeProperties_url')]"
					},
					"ls_db_covid_cluster_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_db_covid_cluster_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Covid19-DE_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_data_arrived_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_data_arrived_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Covid19-DE_ArmTemplate_0')]"
			]
		}
	]
}